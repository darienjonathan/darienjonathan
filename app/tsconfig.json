{
  // https://v3.nuxtjs.org/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["node"],
    "typeRoots": ["@types", "node_modules/@types"],
    "jsx": "preserve",
    "paths": {
      "~": [
        "."
      ],
      "~/*": [
        "./*"
      ],
      "assets": [
        "assets"
      ],
      "assets/*": [
        "assets/*"
      ],
      "public": [
        "."
      ],
      "public/*": [
        "./*"
      ],
      "#app": [
        "node_modules/nuxt3/dist/app"
      ],
      "#app/*": [
        "node_modules/nuxt3/dist/app/*"
      ],
      "vue-demi": [
        "node_modules/nuxt3/dist/app/compat/vue-demi"
      ],
      "#meta": [
        "node_modules/nuxt3/dist/meta/runtime"
      ],
      "#meta/*": [
        "node_modules/nuxt3/dist/meta/runtime/*"
      ],
      "#build": [
        ".nuxt"
      ],
      "#build/*": [
        ".nuxt/*"
      ],
      "#config": [
        "@nuxt/nitro"
      ],
      "#storage": [
        "@nuxt/nitro"
      ],
      "#assets": [
        "@nuxt/nitro"
      ]
    }
  },
  "compileOnSave": true,
}
